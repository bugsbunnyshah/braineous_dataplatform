{
  "pipeId": "scenario1_pipe",
  "configuration": [
    {
      "stagingStore" : "com.appgallabs.dataplatform.targetSystem.core.driver.MongoDBStagingStore",
      "name": "scenario1_store_mongodb",
      "config": {
        "connectionString": "mongodb://localhost:27017",
        "database": "scenario1_store",
        "collection": "data",
        "jsonpathExpressions": [
          "$.store.book[?(@.price >20)]",
          "$.store.bicycle",
          "$.expensive"
        ]
      }
    }
  ]
}